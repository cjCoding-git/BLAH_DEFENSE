    {
      "description": "Get an attribute from a URL.",
      "fullName": "Get URL attribute",
      "functionType": "StringExpression",
      "name": "URLAttribute",
      "sentence": "",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "eventsFunctionContext.returnValue =",
            "    (new URL(eventsFunctionContext.getArgument(\"URL\")))[eventsFunctionContext.getArgument(\"attr\")];",
            ""
          ],
          "parameterObjects": "",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
      ],
      "expressionType": {
        "type": "string"
      },
      "parameters": [
        {
          "description": "The URL to get the attribute from",
          "name": "URL",
          "type": "string"
        },
        {
          "description": "The attribute to get",
          "name": "attr",
          "supplementaryInformation": "[\"hash\",\"host\",\"hostname\",\"href\",\"origin\",\"password\",\"pathname\",\"port\",\"protocol\",\"search\",\"username\"]",
          "type": "stringWithSelector"
        }
      ],
      "objectGroups": []
    },
    {
      "description": "Gets a parameter of a URL query string.",
      "fullName": "Get a parameter of a URL query string",
      "functionType": "StringExpression",
      "name": "URLQueryStringParameter",
      "sentence": "",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "eventsFunctionContext.returnValue =",
            "    (new URL(eventsFunctionContext.getArgument(\"URL\"))",
            "        .searchParams",
            "        .get(eventsFunctionContext.getArgument(\"param\")));",
            ""
          ],
          "parameterObjects": "",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
      ],
      "expressionType": {
        "type": "string"
      },
      "parameters": [
        {
          "description": "The URL to get a query string parameter from",
          "name": "URL",
          "type": "string"
        },
        {
          "description": "The query string parameter to get",
          "name": "param",
          "type": "string"
        }
      ],
      "objectGroups": []
    },
    {
      "description": "Updates a specific part of a URL.",
      "fullName": "Update a URL attribute",
      "functionType": "StringExpression",
      "name": "ChangeURLAttribute",
      "sentence": "",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const url = new URL(eventsFunctionContext.getArgument(\"URL\"));",
            "url[eventsFunctionContext.getArgument(\"part\")] = eventsFunctionContext.getArgument(\"val\");",
            "eventsFunctionContext.returnValue = url.toString();",
            ""
          ],
          "parameterObjects": "",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
      ],
      "expressionType": {
        "type": "string"
      },
      "parameters": [
        {
          "description": "The URL to change",
          "name": "URL",
          "type": "string"
        },
        {
          "description": "The attribute to update",
          "name": "part",
          "supplementaryInformation": "[\"hash\",\"host\",\"hostname\",\"href\",\"origin\",\"password\",\"pathname\",\"port\",\"protocol\",\"search\",\"username\"]",
          "type": "stringWithSelector"
        },
        {
          "description": "The new value of this attribute",
          "name": "val",
          "type": "string"
        }
      ],
      "objectGroups": []
    },
    {
      "description": "Sets or replaces a query string parameter of a URL.",
      "fullName": "Change a get parameter of a URL",
      "functionType": "StringExpression",
      "name": "ChangeURLQueryStringParameter",
      "sentence": "",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const url = new URL(eventsFunctionContext.getArgument(\"URL\"));\r",
            "url.searchParams.set(\r",
            "    eventsFunctionContext.getArgument(\"param\"),\r",
            "    eventsFunctionContext.getArgument(\"val\")\r",
            ");\r",
            "eventsFunctionContext.returnValue = url.toString();\r",
            ""
          ],
          "parameterObjects": "",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
      ],
      "expressionType": {
        "type": "string"
      },
      "parameters": [
        {
          "description": "The URL to change",
          "name": "URL",
          "type": "string"
        },
        {
          "description": "The query string parameter to update",
          "name": "param",
          "type": "string"
        },
        {
          "description": "The new value of the query string parameter",
          "name": "val",
          "type": "string"
        }
      ],
      "objectGroups": []
    },
    {
      "description": "Removes a query string parameter from an URL.",
      "fullName": "Remove a get parameter of an URL",
      "functionType": "StringExpression",
      "name": "RemoveURLQueryStringParameter",
      "sentence": "",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const url = new URL(eventsFunctionContext.getArgument(\"URL\"));\r",
            "url.searchParams.delete(eventsFunctionContext.getArgument(\"param\"));\r",
            "eventsFunctionContext.returnValue = url.toString();\r",
            ""
          ],
          "parameterObjects": "",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
      ],
      "expressionType": {
        "type": "string"
      },
      "parameters": [
        {
          "description": "The URL to change",
          "name": "URL",
          "type": "string"
        },
        {
          "description": "The query string parameter to remove",
          "name": "param",
          "type": "string"
        }
      ],
      "objectGroups": []
    }
  ],
