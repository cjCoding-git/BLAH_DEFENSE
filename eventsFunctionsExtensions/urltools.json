      "sentence": "Redirect to _PARAM1_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "location.replace(eventsFunctionContext.getArgument(\"to\"));",
            ""
          ],
          "parameterObjects": "",
